{
  "name": "MathAss Language",
  "scopeName": "source.mathass",
  "fileTypes": [
    ".ma"
  ],
  "repository": {},
  "patterns": [
    {
      "name": "comment.line.semicolon.mathass",
      "match": ";.*$"
    },
    {
      "name": "variable.other.mathass",
      "match": "\\$[a-zA-Z_][a-zA-Z0-9_]*"
    },
    {
      "name": "entity.name.function.mathass",
      "match": "\\b(input|print)\\b"
    },
    {
      "name": "keyword.control",
      "match": "\\b(goto|if)\\b"
    },
    {
      "name": "constant.numeric.mathass",
      "match": "\\b-?[0-9]+(\\.[0-9]+)?\\b"
    },
    {
      "name": "string.quoted.double.mathass",
      "match": "\"[^\"]*\""
    }
  ]
}